  <record id="sp_archery_tracker_log_action" model="ir.actions.act_window">
    <field name="name">Logs</field>
    <field name="res_model">sp.archery.tracker.log</field>
    <field name="view_mode">list,form</field>
  </record>
  <record id="sp_archery_tracker_log_tree" model="ir.ui.view">
    <field name="name">sp.archery.tracker.log.list</field>
    <field name="model">sp.archery.tracker.log</field>
    <field name="arch" type="xml">
      <list string="Logs">
        <field name="name" />
        <field name="user_id" />
        <field name="date" />
        <field name="score" />
        <field name="num_of_tens" />
        <field name="num_of_x" />
      </list>
    </field>
  </record>
  <record id="sp_archery_tracker_log_form" model="ir.ui.view">
    <field name="name">sp.archery.tracker.log.form</field>
    <field name="model">sp.archery.tracker.log</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <group>
              <field name="name" />
              <field name="user_id" />
              <field name="date" />
            </group>
            <group>
              <field name="score" />
              <field name="num_of_tens" />
              <field name="num_of_x" />
            </group>
          </group>
        </sheet>
        <sheet>
          <field name="ends" widget="ends" /> 
        </sheet>
      </form>
    </field>
  </record>
</odoo>
