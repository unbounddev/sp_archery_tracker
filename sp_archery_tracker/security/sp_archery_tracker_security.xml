<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record model="res.groups" id="group_archer">
      <field name="name">Role / Archer</field>
  </record>
  <record model="res.groups" id="group_coach">
    <field name="name">Role / Coach</field>
  </record>
  <data noupdate="1">
    
    <record model="ir.rule" id="rule_sp_archery_tracker_log">
      <field name="name">Archers can only modify their own logs</field>
      <field name="model_id" ref="model_sp_archery_tracker_log" />
      <field name="groups" eval="[(4, ref('group_archer'))]" />
      <field name="domain_force">[('user_id', '=', user.id)]</field>
      <field name="perm_create" eval="True" />
      <field name="perm_read" eval="True" />
      <field name="perm_write" eval="True" />
      <field name="perm_unlink" eval="True" />
    </record>
  </data>
</odoo>
